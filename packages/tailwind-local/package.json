    "./postcss.config.js": "./postcss.config.js",
    "./tailwind.config": "./tailwind.config.js",
    "./tailwind.config.js": "./tailwind.config.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.2"
  },
  "peerDependencies": {
    "autoprefixer": ">=10",
    "postcss": ">=8",
    "postcss-import": ">=14",
    "tailwindcss": ">=3"
  }
